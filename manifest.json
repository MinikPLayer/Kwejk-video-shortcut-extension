{
  "description": "Adds a page action to toggle applying CSS to pages.",
  "manifest_version": 2,
  "name": "Kwejk Video Shortcut",
  "version": "1.0",
  "homepage_url": "https://github.com/mdn/webextensions-examples/tree/master/apply-css",

  "background": {
    "scripts": ["kd-bg.js"]
  },

  "browser_action": {
    "default_icon": "icons/border-48.svg",
    "browser_style": true
  },

  "permissions": [
    "notifications",
    "*://kwejk.pl/*"
  ]

}